<div class = 'sidebar'>
	<% if @object %>
		<% if @object.selector == Selector::PROVIDER %>	
			<%= link_to "New Invoice", new_invoice_path(:provider_id => @object.id) %>
			<%= link_to "Provider Fees", invoice_maintenance_index_path(:provider_id => @object.id) %>
			<%= link_to "Provider", provider_path(:id => @object.id) %>
			<%= link_to "Back to Index", invoices_path(:provider_id => @object.id) if @back_index %>
		<% else %>
			<%= link_to "New Invoice", new_invoice_path(:group_id => @object.id) %>
			<%= link_to "Group Fees", invoice_maintenance_index_path(:group_id => @object.id)%>
			<%= link_to "Group", group_path(:id => @object.id) %>
						<%= link_to "Back to Index", invoices_path(:group_id => @object.id) if @back_index %>
		<% end %>
	<% elsif @back_index %>
		<%= link_to "Back to Index", invoices_path() %>
	<% end %>
	<br />
</div>